# AWS S3 Configuration
# Your S3 bucket URL for file storage
S3_BUCKET_NAME=your-bucket-name

# AWS Credentials
# Access key ID for AWS services
AWS_ACCESS_KEY_ID=your_access_key_id_here
# Secret access key for AWS services
AWS_SECRET_ACCESS_KEY=your_secret_key_here
# AWS region for your services
AWS_REGION=your_region_here

# Database Configuration
# PostgreSQL connection string for your database
DATABASE_URL=postgres://username:password@host:port/database_name

# Redis Configuration
# Redis connection URL (defaults to localhost if not set)
REDIS_URL=redis://localhost:6379

# Google reCAPTCHA Configuration
# Site key for reCAPTCHA v3 (public key)
RECAPTCHA_SITE_KEY=your_recaptcha_site_key_here
# Secret key for reCAPTCHA v3 (private key)
RECAPTCHA_SECRET_KEY=your_recaptcha_secret_key_here

# Slack Integration
# Webhook URL for sending notifications to Slack channel
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR_WORKSPACE_ID/YOUR_CHANNEL_ID/YOUR_WEBHOOK_TOKEN

# PostHog Analytics Configuration
# PostHog project API key for analytics tracking
POSTHOG_KEY=your_posthog_project_key_here
# PostHog host URL for analytics endpoint
POSTHOG_HOST=https://us.i.posthog.com